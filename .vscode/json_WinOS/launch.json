    "version": "0.2.0",
    "configurations": [
        {
            "name": "gcc - Build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/push_swap",
            "args": [
                // "2",
                // "1",
                // "4",
                // "-6"
				"30 46 37 16 1 43 29 7 11 20 42 10 38 33 8 17 21 50 23 9"
				// 5 2 3 4 -6 99 36 -5 7 365321 89 -1328 1 -8934
				// 5 2 3 4 -6 99 36 -5 7 365321 89 -1328 1 -8934 -77  15 -33 18 
				// // 
				// "5",  
				// "2",  
				// "3",  
				// "4",  
				// "-6",  
				// "99",  
				// "36",  
				// "-5",  
				// "7",  
				// "365321",  
				// "89",  
				// "-1328",  
				// "1",  
				// "-8934"
            ],
            "stopAtEntry": true,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: gcc build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}